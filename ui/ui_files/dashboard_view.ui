  <property name="styleSheet">
   <string notr="true">/* Main Window Dark Theme */
QMainWindow {
    background-color: #1e1e1e;
    color: #e0e0e0;
}

/* Central Widget */
QWidget#centralwidget {
    background-color: #1e1e1e;
    color: #e0e0e0;
}

/* Frame Styling */
QFrame {
    background-color: #2b2b2b;
    border: 1px solid #555555;

}

QFrame[frameShape=&quot;4&quot;] { /* StyledPanel */
    background-color: #2b2b2b;
    border: 1px solid #555555;
}

QFrame[frameShadow=&quot;16&quot;] { /* Raised */
    background-color: #3c3c3c;
    border: 1px solid #666666;
}

QFrame[frameShadow=&quot;32&quot;] { /* Sunken */
    background-color: #252525;
    border: 1px solid #444444;
}

/* Label Styling */
QLabel {
    background-color: transparent;
    color: #e0e0e0;
    border: none;
}

QLabel[font-weight=&quot;75&quot;] {
    color: #ffffff;
}

/* Table Widget Styling */
QTableWidget {
    background-color: #2b2b2b;
    alternate-background-color: #3c3c3c;
    color: #e0e0e0;
    gridline-color: #555555;
    border: 1px solid #555555;
    selection-background-color: #4a90e2;
    selection-color: #ffffff;
}

QTableWidget::item {
    padding: 4px;
    border: none;
}

QTableWidget::item:selected {
    background-color: #4a90e2;
    color: #ffffff;
}

QHeaderView::section {
    background-color: #3c3c3c;
    color: #e0e0e0;
    padding: 6px;
    border: 1px solid #555555;
    font-weight: bold;
}

QHeaderView::section:hover {
    background-color: #4a90e2;
    color: #ffffff;
}

/* Tab Widget Styling */
QTabWidget::pane {
    border: 1px solid #555555;
    background-color: #2b2b2b;
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar::tab {
    background-color: #3c3c3c;
    color: #e0e0e0;
    padding: 8px 16px;
    margin-right: 2px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border: 1px solid #555555;
    border-bottom: none;
}

QTabBar::tab:selected {
    background-color: #2b2b2b;
    color: #ffffff;
    border-bottom: 1px solid #2b2b2b;
}

QTabBar::tab:hover {
    background-color: #4a90e2;
    color: #ffffff;
}

/* Line Edit Styling */
QLineEdit {
    background-color: #3c3c3c;
    color: #e0e0e0;
    border: 1px solid #555555;

    padding: 4px;
}

QLineEdit:focus {
    border: 1px solid #4a90e2;
    background-color: #404040;
}

QLineEdit::placeholder {
    color: #888888;
}

/* Combo Box Styling */
QComboBox {
    background-color: #3c3c3c;
    color: #e0e0e0;
    border: 1px solid #555555;

    padding: 4px;
    min-width: 6em;
}

QComboBox:hover {
    border: 1px solid #4a90e2;
}

QComboBox:focus {
    border: 1px solid #4a90e2;
    background-color: #404040;
}

QComboBox::drop-down {
    border: none;
    width: 20px;
}

QComboBox::down-arrow {
    image: none;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 5px solid #e0e0e0;
    margin-right: 5px;
}

QComboBox QAbstractItemView {
    background-color: #3c3c3c;
    color: #e0e0e0;
    border: 1px solid #555555;
    selection-background-color: #4a90e2;
    selection-color: #ffffff;
}

/* Push Button Styling */
QPushButton {
    background-color: #4a90e2;
    color: #ffffff;
    border: 1px solid #3a80d2;

    padding: 4px 8px;
    font-weight: normal;
}

QPushButton:hover {
    background-color: #5ba0f2;
    border: 1px solid #4a90e2;
}

QPushButton:pressed {
    background-color: #3a80d2;
    border: 1px solid #2a70c2;
}

QPushButton:disabled {
    background-color: #555555;
    color: #888888;
    border: 1px solid #444444;
}

/* Text Edit Styling */
QTextEdit {
    background-color: #2b2b2b;
    color: #e0e0e0;
    border: 1px solid #555555;

    padding: 4px;
}

QTextEdit:focus {
    border: 1px solid #4a90e2;
}

/* Menu Bar Styling */
QMenuBar {
    background-color: #2b2b2b;
    color: #e0e0e0;
    border-bottom: 1px solid #555555;
}

QMenuBar::item {
    background-color: transparent;
    padding: 6px 10px;
}

QMenuBar::item:selected {
    background-color: #4a90e2;
    color: #ffffff;
}

QMenuBar::item:pressed {
    background-color: #3a80d2;
}

/* Menu Styling */
QMenu {
    background-color: #2b2b2b;
    color: #e0e0e0;
    border: 1px solid #555555;
    padding: 4px;
}

QMenu::item {
    padding: 6px 20px;
    border: none;
}

QMenu::item:selected {
    background-color: #4a90e2;
    color: #ffffff;
}

QMenu::separator {
    height: 1px;
    background-color: #555555;
    margin: 4px 0px;
}

/* Status Bar Styling */
QStatusBar {
    background-color: #2b2b2b;
    color: #e0e0e0;
    border-top: 1px solid #555555;
}

QStatusBar::item {
    border: none;
}

/* Splitter Styling */
QSplitter::handle {
    background-color: #555555;
}

QSplitter::handle:horizontal {
    width: 2px;
}

QSplitter::handle:vertical {
    height: 2px;
}

QSplitter::handle:hover {
    background-color: #4a90e2;
}

/* Web Engine View Styling */
QWebEngineView {
    background-color: #1e1e1e;
    border: 1px solid #555555;
}

/* Scroll Bar Styling */
QScrollBar:vertical {
    background-color: #2b2b2b;
    width: 12px;

}

QScrollBar::handle:vertical {
    background-color: #555555;

    min-height: 20px;
}

QScrollBar::handle:vertical:hover {
    background-color: #4a90e2;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar:horizontal {
    background-color: #2b2b2b;
    height: 12px;

}

QScrollBar::handle:horizontal {
    background-color: #555555;

    min-width: 20px;
}

QScrollBar::handle:horizontal:hover {
    background-color: #4a90e2;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    width: 0px;
}</string>
