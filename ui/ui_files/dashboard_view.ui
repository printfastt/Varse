   <string notr="true">/* ðŸš€ VARSE CYBERPUNK FINANCIAL TERMINAL ðŸš€ */
/* Main Window - Deep Space Theme */
QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, 
                stop:0 #0a0a0a, stop:0.3 #161616, stop:0.7 #1a1a2e, stop:1 #0f0f23);
    color: #ffffff;
    font-family: 'Segoe UI', 'Inter', sans-serif;
}

/* Central Widget - Transparent for gradient background */
QWidget#centralwidget {
    background: transparent;
    color: #ffffff;
}

/* Frame Styling - Glass Morphism */
QFrame {
    background: rgba(25, 25, 35, 0.7);
    border: 1px solid rgba(0, 255, 255, 0.3);
    border-radius: 12px;
}

QFrame[frameShape=&quot;4&quot;] { /* StyledPanel - Glass Cards */
    background: rgba(25, 25, 35, 0.8);
    border: 1px solid rgba(0, 255, 255, 0.4);
    border-radius: 12px;
}

QFrame[frameShadow=&quot;16&quot;] { /* Raised - Elevated Glass */
    background: rgba(35, 35, 45, 0.9);
    border: 1px solid rgba(0, 255, 255, 0.5);
    border-radius: 12px;
}

QFrame[frameShadow=&quot;32&quot;] { /* Sunken - Inset Glass */
    background: rgba(15, 15, 25, 0.9);
    border: 1px solid rgba(255, 0, 128, 0.3);
    border-radius: 8px;
}

/* Label Styling - Cyberpunk Text */
QLabel {
    background-color: transparent;
    color: #ffffff;
    border: none;
    font-weight: 400;
    letter-spacing: 0.3px;
}

QLabel[font-weight=&quot;75&quot;] {
    color: #00ffff;
    text-shadow: 0 0 8px rgba(0, 255, 255, 0.6);
    font-weight: 600;
}

/* Table Widget Styling - Cyberpunk Glass */
QTableWidget {
    background: rgba(15, 15, 25, 0.8);
    alternate-background-color: rgba(25, 25, 35, 0.6);
    color: #ffffff;
    gridline-color: rgba(0, 255, 255, 0.2);
    border: 1px solid rgba(0, 255, 255, 0.4);
    border-radius: 8px;
    selection-background-color: rgba(0, 255, 255, 0.3);
    selection-color: #ffffff;
    font-family: 'Consolas', 'JetBrains Mono', monospace;
}

QTableWidget::item {
    padding: 8px;
    border: none;
    border-bottom: 1px solid rgba(0, 255, 255, 0.1);
}

QTableWidget::item:selected {
    background: rgba(0, 255, 255, 0.2);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.6);
}

QTableWidget::item:hover {
    background: rgba(0, 255, 255, 0.1);
    border: 1px solid rgba(0, 255, 255, 0.4);
}

QHeaderView::section {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                stop:0 rgba(35, 35, 45, 0.9), stop:1 rgba(45, 45, 55, 0.9));
    color: #00ffff;
    padding: 10px;
    border: 1px solid rgba(0, 255, 255, 0.3);
    font-weight: 600;
    font-size: 11px;
    text-transform: uppercase;
    letter-spacing: 1px;
}

QHeaderView::section:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                stop:0 rgba(0, 255, 255, 0.3), stop:1 rgba(0, 255, 255, 0.1));
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.8);
}

/* Tab Widget Styling - Floating Glass Pills */
QTabWidget::pane {
    border: 1px solid rgba(0, 255, 255, 0.3);
    background: rgba(15, 15, 25, 0.8);
    border-radius: 12px;
}

QTabWidget::tab-bar {
    alignment: left;
}

QTabBar::tab {
    background: rgba(25, 25, 35, 0.7);
    color: #a1a1aa;
    padding: 10px 20px;
    margin-right: 4px;
    margin-top: 4px;
    border-radius: 20px;
    border: 1px solid rgba(0, 255, 255, 0.2);
    font-weight: 500;
    min-width: 80px;
    text-align: center;
}

QTabBar::tab:selected {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                stop:0 rgba(0, 255, 255, 0.3), stop:1 rgba(139, 92, 246, 0.3));
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.8);
    text-shadow: 0 0 8px rgba(0, 255, 255, 0.6);
    font-weight: 600;
}

QTabBar::tab:hover {
    background: rgba(0, 255, 255, 0.2);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.6);
    transform: translateY(-1px);
}

/* Line Edit Styling - Neon Input Fields */
QLineEdit {
    background: rgba(15, 15, 25, 0.8);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.3);
    border-radius: 8px;
    padding: 8px 12px;
    font-family: 'Consolas', 'JetBrains Mono', monospace;
    font-weight: 500;
    selection-background-color: rgba(0, 255, 255, 0.3);
}

QLineEdit:focus {
    border: 2px solid rgba(0, 255, 255, 0.8);
    background: rgba(20, 20, 30, 0.9);
    box-shadow: 0 0 15px rgba(0, 255, 255, 0.4);
}

QLineEdit:hover {
    border: 1px solid rgba(0, 255, 255, 0.6);
    background: rgba(20, 20, 30, 0.8);
}

QLineEdit::placeholder {
    color: #52525b;
    font-style: italic;
}

/* Combo Box Styling - Futuristic Dropdown */
QComboBox {
    background: rgba(15, 15, 25, 0.8);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.3);
    border-radius: 8px;
    padding: 8px 12px;
    min-width: 80px;
    font-family: 'Consolas', 'JetBrains Mono', monospace;
    font-weight: 500;
}

QComboBox:hover {
    border: 1px solid rgba(0, 255, 255, 0.6);
    background: rgba(20, 20, 30, 0.8);
}

QComboBox:focus {
    border: 2px solid rgba(0, 255, 255, 0.8);
    background: rgba(20, 20, 30, 0.9);
    box-shadow: 0 0 12px rgba(0, 255, 255, 0.3);
}

QComboBox::drop-down {
    border: none;
    width: 25px;
    border-radius: 4px;
}

QComboBox::down-arrow {
    image: none;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-top: 6px solid #00ffff;
    margin-right: 8px;
}

QComboBox::down-arrow:hover {
    border-top: 6px solid #ffffff;
}

QComboBox QAbstractItemView {
    background: rgba(15, 15, 25, 0.95);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.6);
    border-radius: 8px;
    selection-background-color: rgba(0, 255, 255, 0.3);
    selection-color: #ffffff;
    outline: none;
}

/* Push Button Styling - Neon Action Buttons */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                stop:0 rgba(0, 255, 255, 0.8), stop:1 rgba(139, 92, 246, 0.8));
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.6);
    border-radius: 8px;
    padding: 10px 16px;
    font-weight: 600;
    font-size: 11px;
    text-transform: uppercase;
    letter-spacing: 0.5px;
}

QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                stop:0 rgba(0, 255, 255, 1.0), stop:1 rgba(139, 92, 246, 1.0));
    border: 1px solid rgba(0, 255, 255, 1.0);
    box-shadow: 0 0 20px rgba(0, 255, 255, 0.6);
    transform: translateY(-1px);
}

QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                stop:0 rgba(0, 200, 200, 0.9), stop:1 rgba(100, 60, 200, 0.9));
    border: 1px solid rgba(0, 200, 200, 0.8);
    transform: translateY(0px);
}

QPushButton:disabled {
    background: rgba(55, 55, 55, 0.5);
    color: #52525b;
    border: 1px solid rgba(85, 85, 85, 0.3);
    box-shadow: none;
}

/* Text Edit Styling - Glass Content Areas */
QTextEdit {
    background: rgba(15, 15, 25, 0.8);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.3);
    border-radius: 8px;
    padding: 12px;
    font-family: 'Segoe UI', sans-serif;
    font-size: 12px;
    line-height: 1.4;
    selection-background-color: rgba(0, 255, 255, 0.3);
}

QTextEdit:focus {
    border: 1px solid rgba(0, 255, 255, 0.6);
    background: rgba(20, 20, 30, 0.9);
    box-shadow: 0 0 12px rgba(0, 255, 255, 0.2);
}

/* Menu Bar Styling - Futuristic Navigation */
QMenuBar {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 rgba(25, 25, 35, 0.9), stop:1 rgba(35, 35, 45, 0.9));
    color: #ffffff;
    border-bottom: 1px solid rgba(0, 255, 255, 0.4);
    font-weight: 500;
}

QMenuBar::item {
    background: transparent;
    padding: 8px 16px;
    border-radius: 4px;
    margin: 2px;
}

QMenuBar::item:selected {
    background: rgba(0, 255, 255, 0.2);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.5);
}

QMenuBar::item:pressed {
    background: rgba(0, 255, 255, 0.3);
    color: #ffffff;
}

/* Menu Styling - Glass Dropdown */
QMenu {
    background: rgba(15, 15, 25, 0.95);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.6);
    border-radius: 8px;
    padding: 8px;
}

QMenu::item {
    padding: 8px 24px;
    border: none;
    border-radius: 4px;
    margin: 1px;
}

QMenu::item:selected {
    background: rgba(0, 255, 255, 0.3);
    color: #ffffff;
    border: 1px solid rgba(0, 255, 255, 0.6);
}

QMenu::separator {
    height: 1px;
    background: rgba(0, 255, 255, 0.3);
    margin: 6px 12px;
}

/* Status Bar Styling - Futuristic Footer */
QStatusBar {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                stop:0 rgba(25, 25, 35, 0.9), stop:1 rgba(35, 35, 45, 0.9));
    color: #ffffff;
    border-top: 1px solid rgba(0, 255, 255, 0.4);
    font-size: 11px;
    font-weight: 500;
}

QStatusBar::item {
    border: none;
    padding: 4px 8px;
}

/* Splitter Styling - Neon Dividers */
QSplitter::handle {
    background: rgba(0, 255, 255, 0.3);
}

QSplitter::handle:horizontal {
    width: 3px;
    margin: 2px 0px;
    border-radius: 1px;
}

QSplitter::handle:vertical {
    height: 3px;
    margin: 0px 2px;
    border-radius: 1px;
}

QSplitter::handle:hover {
    background: rgba(0, 255, 255, 0.8);
    box-shadow: 0 0 8px rgba(0, 255, 255, 0.6);
}

/* Web Engine View Styling - Chart Containers */
QWebEngineView {
